---
title: Home | IntelliFare
layout: "base.njk"
import: "/contaner/command/index"
---

<!-- Main Content-->
<main id="main-content">
  {%- for post in collections.homepage -%}
  <!-- First container - Welcome Section-->
  <section id="welcome-section" data-aos="fade-in">
    <h1 id="header-title">IntelliFare</h1>
    <p>Redefining Transportation, One Fare at a Time</p>
    <a href="/product">
      <button>Get Started</button>
    </a>
  </section>
  <!-- /First container - Welcome Section-->

  <!-- Second container-->
  <section class="article-section" data-aos="fade-in">
    <div>
      <a href="{{ post.data.first_article_link }}">
        <img
          src="{{ post.data.first_article_image }}"
          alt="{{ post.data.first_article_image_alternative }}"
          loading="lazy"
        />
      </a>
    </div>
    <div class="article-content-text">
      <h2>{{ post.data.first_article_name }}</h2>
      <p>
        {{ post.data.first_article_description }}
      </p>
      <a href="{{ post.data.first_article_link }}">
        <button>More</button>
      </a>
    </div>
  </section>
  <!-- /Second container-->

  <!-- Third container-->
  <section class="article-section" data-aos="fade-in">
    <div>
      <a href="{{ post.data.second_article_link }}">
        <img
          src="{{ post.data.second_article_image }}"
          alt="{{ post.data.second_article_image_alternative }}"
          loading="lazy"
        />
      </a>
    </div>
    <div class="article-content-text">
      <h2>{{ post.data.second_article_name }}</h2>
      <p>
        {{ post.data.second_article_description }}
      </p>
      <a href="{{ post.data.second_article_link }}">
        <button>More</button>
      </a>
    </div>
  </section>
  <!-- /Third container-->
  {%- endfor -%}
</main>
<!-- / Main Content-->
